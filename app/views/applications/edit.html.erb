application page

<p>SECTION A: PERSONAL DATA </p>

<%= form_for(@application) do |f| %>
  <p><%= label_tag :last_name %></p>
  <p><%= text_field_tag :'user[last_name]', current_user.last_name %></p>
  <p><%= label_tag :first_name %></p>
  <p><%= text_field_tag :'user[first_name]', current_user.first_name %></p>
  <p><%= label_tag :middle_initial %></p>
  <p><%= text_field_tag :'user[middle_initial]', current_user.middle_initial %></p>
  <p><%= f.label :phone_number %></p>
  <p><%= f.text_field :phone_number %></p>
  <p><%= f.label :street %></p>
  <p><%= f.text_field :street %></p>
  <p><%= f.label :city %></p>
  <p><%= f.text_field :city %></p>
  <p><%= f.label :state %></p>
  <p><%= f.text_field :state %></p>
  <p><%= f.label :zip_code %></p>
  <p><%= f.text_field :zip_code %></p>
  <p><%= f.label :country %></p>
  <p><%= f.text_field :country %></p>
  <p><%= f.label :birth_place %></p>
  <p><%= f.text_field :birth_place %></p>
  <p><%= f.label :birth_date %></p>
  <p><%= f.text_field :birth_date %></p>
  <p><%= f.label :country_of_citizenship %></p>
  <p><%= f.text_field :country_of_citizenship %></p>
  <p><%= f.label :occupation %></p>
  <p><%= f.text_field :occupation %></p>
  <p><%= f.label :name_of_spouse %></p>
  <p><%= f.text_field :name_of_spouse %></p>
  <p><%= f.label :ages_of_children %></p>
  <p><%= f.text_field :ages_of_children %></p>
  <p><%= label_tag :email %></p>
  <p><%= text_field_tag :'user[email]', current_user.email, disabled: true %></p>
  <p><%= f.submit :save %></p>
<% end %>





