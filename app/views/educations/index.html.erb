<p>SECTION B: SUPPORTING DATA </p>

<% @educations.each do |education| %>
  <p>School: <%= education.school %></p>
  <p>Location: <%= education.location %></p>
  <p>Start Date: <%= education.start_date %></p>
  <p>End Date: <%= education.end_date %></p>
  <p>Degree: <%= education.degree %></p>
  <p>Degree date: <%= education.degree_date %></p>
  <p><%= link_to 'Edit education', "/applications/#{@application.id}/educations/#{education.id}/edit"%>
  <hr>
<% end %>

<a href="/applications/<%= @application.id %>/educations/new">Add new document</a>

